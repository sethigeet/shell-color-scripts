#!/bin/sh

# ANSI Color -- use these variables to easily have different color
#    and format output. Make sure to output the reset sequence after
#    colors (f = foreground, b = background), and use the 'off'
#    feature for anything you turn on.

esc=""

redf="${esc}[31m"
greenf="${esc}[32m"
yellowf="${esc}[33m"
bluef="${esc}[34m"
purplef="${esc}[35m"
cyanf="${esc}[36m"

redfbright="${esc}[91m"
greenfbright="${esc}[92m"
yellowfbright="${esc}[93m"
bluefbright="${esc}[94m"
purplefbright="${esc}[95m"
cyanfbright="${esc}[96m"

boldon="${esc}[1m"
invon="${esc}[7m"

reset="${esc}[0m"

cat <<EOF

${invon}
${redf}   â–„â–ˆ     â–ˆâ–„    ${greenf}   â–„â–„     â–„â–„    ${yellowf}   â–„â–„     â–„â–„    ${bluef}   â–„â–„     â–„â–„    ${purplef}   â–„â–„     â–„â–„    ${cyanf}   â–„â–ˆ     â–ˆâ–„   
${redf} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${greenf} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${yellowf} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${bluef} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${purplef} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${cyanf} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„ 
${redf} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${greenf} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${yellowf} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${bluef} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${purplef} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${cyanf} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ 
${redf} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${greenf} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${yellowf} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${bluef} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${purplef} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${cyanf} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ 
${redf} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${greenf} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${yellowf} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${bluef} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${purplef} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${cyanf} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€ 
${redf}   â–€â–ˆ     â–ˆâ–€    ${greenf}   â–€â–€     â–€â–€    ${yellowf}   â–€â–€     â–€â–€    ${bluef}   â–€â–€     â–€â–€    ${purplef}   â–€â–€     â–€â–€    ${cyanf}   â–€â–ˆ     â–ˆâ–€   
${boldon}${invon}
${redfbright}   â–„â–ˆ     â–ˆâ–„    ${greenfbright}   â–„â–„     â–„â–„    ${yellowfbright}   â–„â–„     â–„â–„    ${bluefbright}   â–„â–„     â–„â–„    ${purplefbright}   â–„â–„     â–„â–„    ${cyanfbright}   â–„â–ˆ     â–ˆâ–„   
${redfbright} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${greenfbright} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${yellowfbright} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${bluefbright} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${purplefbright} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„  ${cyanfbright} â–„â–ˆâ–€  â–„â–„â–„  â–€â–ˆâ–„ 
${redfbright} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${greenfbright} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${yellowfbright} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${bluefbright} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${purplefbright} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ  ${cyanfbright} â–ˆâ–ˆâ–„â–„â–ˆâ–ˆâ–€â–ˆâ–ˆâ–„â–„â–ˆâ–ˆ 
${redfbright} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${greenfbright} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${yellowfbright} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${bluefbright} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${purplefbright} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ  ${cyanfbright} â–ˆâ–ˆâ–€â–€â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–€â–€â–ˆâ–ˆ 
${redfbright} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${greenfbright} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${yellowfbright} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${bluefbright} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${purplefbright} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€  ${cyanfbright} â–€â–ˆâ–„  â–€â–€â–€  â–„â–ˆâ–€ 
${redfbright}   â–€â–ˆ     â–ˆâ–€    ${greenfbright}   â–€â–€     â–€â–€    ${yellowfbright}   â–€â–€     â–€â–€    ${bluefbright}   â–€â–€     â–€â–€    ${purplefbright}   â–€â–€     â–€â–€    ${cyanfbright}   â–€â–ˆ     â–ˆâ–€   

#!/bin/sh

# ANSI Color -- use these variables to easily have different color
#    and format output. Make sure to output the reset sequence after
#    colors (f = foreground, b = background), and use the 'off'
#    feature for anything you turn on.

esc=""

redf="${esc}[31m"
greenf="${esc}[32m"
yellowf="${esc}[33m"
bluef="${esc}[34m"
purplef="${esc}[35m"
cyanf="${esc}[36m"

redfbright="${esc}[91m"
greenfbright="${esc}[92m"
yellowfbright="${esc}[93m"
bluefbright="${esc}[94m"
purplefbright="${esc}[95m"
cyanfbright="${esc}[96m"

boldon="${esc}[1m"

reset="${esc}[0m"

cat <<EOF

${redf}    â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${greenf}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${yellowf}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${bluef}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${purplef}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${cyanf}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„   
${redf}   â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${greenf}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${yellowf}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${bluef}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${purplef}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${cyanf}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ
${redf}   â–ˆ      â–ˆ    ${greenf}  â–ˆ      â–ˆ    ${yellowf}  â–ˆ      â–ˆ    ${bluef}  â–ˆ      â–ˆ    ${purplef}  â–ˆ      â–ˆ    ${cyanf}  â–ˆ      â–ˆ   
${redf}  â–ˆ  â–„â–€â–€â–„  â–ˆ   ${greenf} â–ˆ  â–„â–€â–€â–„  â–ˆ   ${yellowf} â–ˆ  â–„â–€â–€â–„  â–ˆ   ${bluef} â–ˆ  â–„â–€â–€â–„  â–ˆ   ${purplef} â–ˆ  â–„â–€â–€â–„  â–ˆ   ${cyanf} â–ˆ  â–„â–€â–€â–„  â–ˆ 
${redf} â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${greenf}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${yellowf}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${bluef}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${purplef}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${cyanf}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ

${boldon}
${redfbright}    â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${greenfbright}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${yellowfbright}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${bluefbright}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${purplefbright}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„     ${cyanfbright}   â–„â–ˆâ–ˆâ–ˆâ–ˆâ–„   
${redfbright}   â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${greenfbright}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${yellowfbright}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${bluefbright}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${purplefbright}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ    ${cyanfbright}  â–ˆâ–ˆâ–€â–€â–€â–€â–ˆâ–ˆ
${redfbright}   â–ˆ      â–ˆ    ${greenfbright}  â–ˆ      â–ˆ    ${yellowfbright}  â–ˆ      â–ˆ    ${bluefbright}  â–ˆ      â–ˆ    ${purplefbright}  â–ˆ      â–ˆ    ${cyanfbright}  â–ˆ      â–ˆ   
${redfbright}  â–ˆ  â–„â–€â–€â–„  â–ˆ   ${greenfbright} â–ˆ  â–„â–€â–€â–„  â–ˆ   ${yellowfbright} â–ˆ  â–„â–€â–€â–„  â–ˆ   ${bluefbright} â–ˆ  â–„â–€â–€â–„  â–ˆ   ${purplefbright} â–ˆ  â–„â–€â–€â–„  â–ˆ   ${cyanfbright} â–ˆ  â–„â–€â–€â–„  â–ˆ 
${redfbright} â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${greenfbright}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${yellowfbright}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${bluefbright}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${purplefbright}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ  ${cyanfbright}â–ˆ â–„â–ˆâ–¬â–„â–„â–¬â–ˆâ–„ â–ˆ
${reset}


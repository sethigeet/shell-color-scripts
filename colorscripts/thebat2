#!/bin/sh

# ANSI Color -- use these variables to easily have different color
#    and format output. Make sure to output the reset sequence after
#    colors (f = foreground, b = background), and use the 'off'
#    feature for anything you turn on.

esc=""

redf="${esc}[31m"
greenf="${esc}[32m"
yellowf="${esc}[33m"
bluef="${esc}[34m"
purplef="${esc}[35m"
cyanf="${esc}[36m"

redfbright="${esc}[91m"
greenfbright="${esc}[92m"
yellowfbright="${esc}[93m"
bluefbright="${esc}[94m"
purplefbright="${esc}[95m"
cyanfbright="${esc}[96m"

boldon="${esc}[1m"

reset="${esc}[0m"

cat <<EOF

 ${redf} â–„â–ˆâ–€ â–ˆ â–ˆ â–€â–ˆâ–„   ${greenf} â–„â–ˆâ–€ â–ˆ â–ˆ â–€â–ˆâ–„   ${yellowf} â–„â–ˆâ–€ â–ˆ â–ˆ â–€â–ˆâ–„   ${bluef} â–„â–ˆâ–€ â–ˆ â–ˆ â–€â–ˆâ–„   ${purplef} â–„â–ˆâ–€ â–ˆ â–ˆ â–€â–ˆâ–„   ${cyanf} â–„â–ˆâ–€ â–ˆ â–ˆ â–€â–ˆâ–„ 

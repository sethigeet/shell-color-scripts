#!/bin/sh

# ANSI Color -- use these variables to easily have different color
#    and format output. Make sure to output the reset sequence after
#    colors (f = foreground, b = background), and use the 'off'
#    feature for anything you turn on.

esc=""

redf="${esc}[31m"
greenf="${esc}[32m"
yellowf="${esc}[33m" bluef="${esc}[34m"
purplef="${esc}[35m"
cyanf="${esc}[36m"

redfbright="${esc}[91m"
greenfbright="${esc}[92m"
yellowfbright="${esc}[93m" bluefbright="${esc}[94m"
purplefbright="${esc}[95m"
cyanfbright="${esc}[96m"

boldon="${esc}[1m"
boldoff="${esc}[22m"

reset="${esc}[0m"

cat <<EOF
 
 ${reset}${redf}  â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${greenf}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${yellowf}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${bluef}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${purplef}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${cyanf}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}
 ${reset}${redf}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${reset}${greenf} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${reset}${yellowf} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${reset}${bluef} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${reset}${purplef} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${reset} ${cyanf}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${reset}
 ${reset}${redf}  â–ˆâ–ˆ${boldon}${redfbright}â–ˆâ–ˆ${boldoff}${redf}â–ˆâ–ˆ   ${reset}${greenf}   â–ˆâ–ˆ${boldon}${greenfbright}â–ˆâ–ˆ${boldoff}${greenf}â–ˆâ–ˆ   ${reset}${yellowf}   â–ˆâ–ˆ${boldon}${yellowfbright}â–ˆâ–ˆ${boldoff}${yellowf}â–ˆâ–ˆ   ${reset}${bluef}   â–ˆâ–ˆ${boldon}${bluefbright}â–ˆâ–ˆ${boldoff}${bluef}â–ˆâ–ˆ ${reset}${purplef}     â–ˆâ–ˆ${boldon}${purplefbright}â–ˆâ–ˆ${boldoff}${purplef}â–ˆâ–ˆ   ${reset}${cyanf}   â–ˆâ–ˆ${boldon}${cyanfbright}â–ˆâ–ˆ${boldoff}${cyanf}â–ˆâ–ˆ   ${reset}
 ${reset}${redf}â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${reset}${greenf} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${reset}${yellowf} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${bluef} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${purplef} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ ${reset}${cyanf} â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ${reset}
 ${reset}${redf}  â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${greenf}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${yellowf}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${bluef}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${purplef}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}${cyanf}   â–ˆâ–ˆ  â–ˆâ–ˆ   ${reset}
 ${reset}
EOF
